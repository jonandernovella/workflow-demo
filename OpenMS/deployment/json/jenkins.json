{
	"cpus": 0.5, 
	"mem": 128,
	"id": "jenkins",
	"instances": 1,
	"container": {
    		"type": "DOCKER",
    		"docker": {
      			"image": "jenkins",
      			"network": "BRIDGE",
			"portMappings": [{
                    		"containerPort": 8080,
                    		"hostPort": 0,
                    		"protocol": "tcp"
                	}]
    		}
	}		
}
